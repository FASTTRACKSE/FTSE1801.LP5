<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="address" class="bean.Address">
		<constructor-arg value="Da Nang"></constructor-arg>
		<constructor-arg value="Viet Nam"></constructor-arg>
	</bean>

	<bean id="user" class="bean.User" autowire="constructor">
		<constructor-arg value="1" type="int"/>
		<constructor-arg value="Tran Duy Luan"/>
	</bean>

</beans> 